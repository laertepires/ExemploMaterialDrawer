<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="thiagocury.eti.br.exemplomaterialdrawer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TelaInicial">
        </activity>
        <activity android:name=".Tela1">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TelaInicial" />
        </activity>
        <activity android:name=".Tela2">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".TelaInicial" />
        </activity>
        <activity
            android:name=".TelaAbertura"
            android:label="@string/title_activity_tela_abertura"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>